											<telerik:ChartSeriesLabelDefinition Template="{StaticResource LabelWeaponStatsTemplate}"/>
										</telerik:BarSeries.LabelDefinitions>
									</telerik:BarSeries>
									<telerik:BarSeries CombineMode="Stack"
													   StackGroupKey="tazer"
													   ItemsSource="{Binding DatasTazerDeath}"
													   CategoryBinding="Category"
													   ValueBinding="Value"
													   ShowLabels="True">
										<telerik:BarSeries.LabelDefinitions>
											<telerik:ChartSeriesLabelDefinition Template="{StaticResource LabelWeaponStatsTemplate}" />
										</telerik:BarSeries.LabelDefinitions>
									</telerik:BarSeries>
									<telerik:BarSeries CombineMode="Stack"
													   StackGroupKey="knife"
													   ItemsSource="{Binding DatasKnifeKill}"
													   CategoryBinding="Category"
													   ValueBinding="Value"
													   ShowLabels="True">
										<telerik:BarSeries.LabelDefinitions>
											<telerik:ChartSeriesLabelDefinition Template="{StaticResource LabelWeaponStatsTemplate}" />
										</telerik:BarSeries.LabelDefinitions>
									</telerik:BarSeries>
									<telerik:BarSeries CombineMode="Stack"
													   StackGroupKey="knife"
													   ItemsSource="{Binding DatasKnifeDeath}"
													   CategoryBinding="Category"
													   ValueBinding="Value"
													   ShowLabels="True">
										<telerik:BarSeries.LabelDefinitions>
											<telerik:ChartSeriesLabelDefinition Template="{StaticResource LabelWeaponStatsTemplate}" />
										</telerik:BarSeries.LabelDefinitions>
									</telerik:BarSeries>
								</telerik:RadCartesianChart.Series>
							</telerik:RadCartesianChart>

							<telerik:RadLegend Grid.Row="1"
											   VerticalAlignment="Bottom"
											   HorizontalAlignment="Center">
								<telerik:RadLegend.ItemsPanel>
									<ItemsPanelTemplate>
										<StackPanel Orientation="Horizontal" />
									</ItemsPanelTemplate>
								</telerik:RadLegend.ItemsPanel>
								<telerik:RadLegend.Items>
									<telerik:LegendItemCollection>
										<telerik:LegendItem MarkerFill="{StaticResource GreenBrush}"
															Title="{x:Static properties:Resources.Kills}"/>
										<telerik:LegendItem MarkerFill="{StaticResource RedColorBrush}"
															Title="{x:Static properties:Resources.Deaths}" />
									</telerik:LegendItemCollection>
								</telerik:RadLegend.Items>
							</telerik:RadLegend>
						</Grid>

						<Grid Grid.Column="1">
							<Grid.RowDefinitions>
								<RowDefinition Height="Auto"/>
								<RowDefinition Height="Auto"/>
							</Grid.RowDefinitions>

							<TextBlock Grid.Row="0" 
									   HorizontalAlignment="Center"
									   Style="{StaticResource TitleSettingsStyle}"
									   Text="{x:Static properties:Resources.StuffsThrown}"/>

							<Grid Grid.Row="1">
								<Grid.ColumnDefinitions>
									<ColumnDefinition Width="Auto" />
									<ColumnDefinition Width="Auto" />
								</Grid.ColumnDefinitions>

								<Grid Grid.Column="0">
									<Grid.RowDefinitions>
										<RowDefinition Height="Auto"/>
										<RowDefinition Height="Auto"/>
										<RowDefinition Height="Auto"/>
									</Grid.RowDefinitions>

									<Grid Grid.Row="0"
										  Margin="10">
										<Border BorderBrush="{StaticResource LightWhiteBrush}"
												BorderThickness="1">
											<StackPanel Orientation="Vertical"
														Margin="5">
												<TextBlock Text="{x:Static properties:Resources.Smoke}"
														   Style="{StaticResource StatsTitleStyle}"/>
												<TextBlock Text="{Binding SmokeThrownCount}"
														   Style="{StaticResource StatsValueStyle}"/>
											</StackPanel>
										</Border>
									</Grid>
									<Grid Grid.Row="1"
										  Margin="10">
										<Border BorderBrush="{StaticResource LightWhiteBrush}"
												BorderThickness="1">
											<StackPanel Orientation="Vertical"
														Margin="5">
												<TextBlock Text="{x:Static properties:Resources.Flashbangs}"
														   Style="{StaticResource StatsTitleStyle}"/>
												<TextBlock Text="{Binding FlashbangThrownCount}"
														   Style="{StaticResource StatsValueStyle}"/>
											</StackPanel>
										</Border>
									</Grid>
									<Grid Grid.Row="2"
										  Margin="10">
										<Border BorderBrush="{StaticResource LightWhiteBrush}"
												BorderThickness="1">
											<StackPanel Orientation="Vertical"
														Margin="5">
												<TextBlock Text="{x:Static properties:Resources.HeGrenades}"
														   Style="{StaticResource StatsTitleStyle}"/>
												<TextBlock Text="{Binding HeGrenadeThrownCount}"
														   Style="{StaticResource StatsValueStyle}"/>
											</StackPanel>
										</Border>
									</Grid>
								</Grid>

								<Grid Grid.Column="1">
									<Grid.RowDefinitions>
										<RowDefinition Height="Auto"/>
										<RowDefinition Height="Auto"/>
										<RowDefinition Height="Auto"/>
									</Grid.RowDefinitions>

									<Grid Grid.Row="0"
										  Margin="10">
										<Border BorderBrush="{StaticResource LightWhiteBrush}"
												BorderThickness="1">
											<StackPanel Orientation="Vertical"
														Margin="5">
												<TextBlock Text="{x:Static properties:Resources.Molotovs}"
														   Style="{StaticResource StatsTitleStyle}"/>
												<TextBlock Text="{Binding MolotovThrownCount}"
														   Style="{StaticResource StatsValueStyle}"/>
											</StackPanel>
										</Border>
									</Grid>
									<Grid Grid.Row="1"
										  Margin="10">
										<Border BorderBrush="{StaticResource LightWhiteBrush}"
												BorderThickness="1">
											<StackPanel Orientation="Vertical"
														Margin="5">
												<TextBlock Text="{x:Static properties:Resources.Incendiaries}"
														   Style="{StaticResource StatsTitleStyle}"/>
												<TextBlock Text="{Binding IncendiaryThrownCount}"
														   Style="{StaticResource StatsValueStyle}"/>
											</StackPanel>
										</Border>
									</Grid>
									<Grid Grid.Row="2"
										  Margin="10">
										<Border BorderBrush="{StaticResource LightWhiteBrush}"
												BorderThickness="1">
											<StackPanel Orientation="Vertical"
														Margin="5">
												<TextBlock Text="{x:Static properties:Resources.Decoys}"
														   Style="{StaticResource StatsTitleStyle}"/>
												<TextBlock Text="{Binding DecoyThrownCount}"
														   Style="{StaticResource StatsValueStyle}"/>
											</StackPanel>
										</Border>
									</Grid>
								</Grid>
							</Grid>
						</Grid>
					</Grid>
				</controls:MetroTabItem>
			</TabControl>
		</Grid>
	</Grid>
</UserControl>
