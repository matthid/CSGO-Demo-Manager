	<ContextMenu x:Key="DemoMovieRoundContextMenu"
				 d:DataContext="{d:DesignInstance timelines:TimelineEvent}">
		<MenuItem Command="{Binding DemoMovie.UpdateStartTick, Source={StaticResource Locator}}"
				  CommandParameter="{Binding DataItem.StartTick}">
			<MenuItem.Header>
				<TextBlock Text="{Binding Path=DataItem.RoundStartEventName, StringFormat={x:Static properties:Resources.ContextMenuTimeLineSetEventTickAsStartTick}}" />
			</MenuItem.Header>
		</MenuItem>
		<MenuItem Command="{Binding DemoMovie.UpdateStartTickBeforeSeconds, Source={StaticResource Locator}}"
				  CommandParameter="{Binding DataItem.StartTick}">
			<MenuItem.Header>
				<TextBlock Text="{Binding Path=DataItem.RoundStartEventName, StringFormat={x:Static properties:Resources.ContextMenuTimeLineSetEventTickMinusAsStartTick}}" />
			</MenuItem.Header>
		</MenuItem>
		<MenuItem Command="{Binding DemoMovie.UpdateStartTickAfterSeconds, Source={StaticResource Locator}}"
				  CommandParameter="{Binding DataItem.StartTick}">
			<MenuItem.Header>
				<TextBlock Text="{Binding Path=DataItem.RoundStartEventName, StringFormat={x:Static properties:Resources.ContextMenuTimeLineSetEventTickPlusAsStartTick}}" />
			</MenuItem.Header>
		</MenuItem>
		<Separator />
		<MenuItem Command="{Binding DemoMovie.UpdateEndTick, Source={StaticResource Locator}}"
				  CommandParameter="{Binding DataItem.EndTick}">
