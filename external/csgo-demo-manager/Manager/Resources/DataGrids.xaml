	<!--DataGrid for suspects list style -->
	<Style TargetType="DataGridRow"
		   x:Key="DataGridRowSuspectsListStyle"
		   BasedOn="{StaticResource MetroDataGridRow}">
		<Style.Resources>
			<converters:IntToBooleanConverter x:Key="IntToBooleanConverter" />
		</Style.Resources>
		<Style.Triggers>
			<DataTrigger Binding="{Binding VacBanned}"
						 Value="true">
				<Setter Property="Background"
						Value="{StaticResource RedColorSemiOpaqueBrush}"/>
			</DataTrigger>
			<DataTrigger Binding="{Binding GameBanCount, Converter={StaticResource IntToBooleanConverter}, ConverterParameter=GameBanCount}" Value="true">
				<Setter Property="Background"
						Value="{StaticResource RedColorSemiOpaqueBrush}"/>
			</DataTrigger>
		</Style.Triggers>
		<Setter Property="ContextMenu"
				Value="{StaticResource SuspectRowContextMenu}" />
	</Style>

