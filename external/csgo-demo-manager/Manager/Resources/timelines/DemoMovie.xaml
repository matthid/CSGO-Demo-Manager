						   Text="{Binding StartTick, StringFormat='{}{0}:'}" />
				<TextBlock Style="{StaticResource TimeLineToolTipTextBlockStyle}"
						   Foreground="{StaticResource GreenBrush}"
						   Text="{Binding ThrowerName, StringFormat=' \{0}\ '}" />
				<TextBlock Style="{StaticResource TimeLineToolTipTextBlockStyle}"
						   Text="{x:Static properties:Resources.Thrown}" />
				<TextBlock Style="{StaticResource TimeLineToolTipTextBlockStyle}"
						   Foreground="{StaticResource RedColorBrush}"
						   Text="{Binding StuffType, Converter={StaticResource StuffTypeToolTipToStringConverter}, StringFormat=' \{0}'}" />
			</StackPanel>
		</Grid>
	</DataTemplate>

	<DataTemplate x:Key="DemoTimeLineRoundToolTip">
		<Grid d:DataContext="{d:DesignInstance timelines:RoundEventTimeline}">
			<Rectangle Margin="0"
					   Fill="{DynamicResource HighlightBrush}" />
			<Rectangle Margin="2"
					   Fill="{DynamicResource WhiteBrush}"/>
			<StackPanel Orientation="Horizontal" Margin="5">
				<TextBlock Style="{StaticResource TimeLineToolTipTextBlockStyle}"
						   Text="{x:Static properties:Resources.RoundNumber}" />
				<TextBlock Style="{StaticResource TimeLineToolTipTextBlockStyle}"
						   Foreground="{StaticResource GreenBrush}"
						   Text="{Binding Number, StringFormat='{}{0} '}" />
				<TextBlock Style="{StaticResource TimeLineToolTipTextBlockStyle}"
						   Foreground="{StaticResource RedColorBrush}"
						   Text="{Binding StartTick, StringFormat=' \{0}\ '}" />
				<TextBlock Text="-" Style="{StaticResource TimeLineToolTipTextBlockStyle}" />
				<TextBlock Style="{StaticResource TimeLineToolTipTextBlockStyle}"
						   Foreground="{StaticResource RedColorBrush}"
						   Text="{Binding EndTick, StringFormat=' \{0}\ '}" />
				<TextBlock Style="{StaticResource TimeLineToolTipTextBlockStyle}"
						   Text="{Binding Duration.TotalSeconds, StringFormat='{}({0:N0}s)'}" />
			</StackPanel>
		</Grid>
	</DataTemplate>

